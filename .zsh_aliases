alias v='ls --color=auto -Fal'
alias cp='nocorrect cp -v'
alias mv='nocorrect mv -v'
alias vi='nocorrect vi -p'
alias grep='grep --color=auto'
alias ...='../..'

# Send Android input events from the commandline over adb
alias akey='adb shell input keyevent'
alias atext='adb shell input text'

alias -s h='vi -p'
alias -s cpp='vi -p'
alias -s java='vi -p'
alias -s mk='vi -p'
